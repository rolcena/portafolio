function Mostrar_Mensajero(){
	document.getElementById('dsdpy-mensajero-2').style.display='inline';

	document.getElementById('div-mostrar-mensajero').style.display='none';
}

function Mostrar_Principales(){
	//if(radio=="780" || radio=="970" || radio=="1080" || radio=="780-endiferido" || radio=="1080-endiferido" || radio=="970-endiferido"){
	radio_sql=radio.replace("-endiferido","");
	if(diferido[radio_sql]=="1"){
		var split = radio.split('-');
		var radio_aux = split[0];
		document.getElementById('endiferido').style.display='inline';
		document.getElementById('endiferido').style.visibility='visible';
		//document.getElementById('endiferido-iframe').innerHTML='<iframe src="http://www.desdeparaguay.net/listaProgramas.aspx?medio='+radio_aux+'" width="336" height="160" frameborder="0" scrolling="yes">';
		document.getElementById('endiferido-iframe').innerHTML='<iframe src="https://www.desdepylabs.com/Webutils/listaProgramas/'+radio_aux+'" style="background-color:#ffffff;" allowTransparency="false" width="336" height="120" frameborder="0" scrolling="yes">';
	}
}

function Live_Cam(valor){
	if(webcam[valor]!="0"){
		document.getElementById('dsdpy-webcam').innerHTML='';
		//camara=valor+webcam[valor];
		camara=webcam[valor];
		if(radio.substr(radio.length-11)!='-endiferido'){
			document.getElementById('dsdpy-webcam').style.display='inline';
			Flash('dsdpy-webcam',336,271,'center','666&radio='+valor+'&camara='+camara);
		}
	}else{
		document.getElementById('dsdpy-webcam').innerHTML='';
	}


				//document.getElementById('tw-radio').href='https://twitter.com/E40PY';
		//document.getElementById('tw-radio').innerHTML='<a class="twitter-timeline" data-lang="es" data-theme="light" data-height="600" data-link-color="#87b018" href="https://twitter.com/dsdpy"></a>';
		//alert("WWelcome");
		//alert(twitter[valor]);
		if(twitter[valor]!=""){
			//alert("Welcome");
			document.getElementById('tw-radio').innerHTML='<iframe src="https://www.desdeparaguay.com/tw.php?'+twitter[valor]+'" width="336" height="600" frameborder="0" scrolling="no"></iframe>';
		}else{
			document.getElementById('tw-radio').innerHTML='';
		}
		if(facebook[valor]!=""){
			//alert("Welcome");
			document.getElementById('fb-radio').innerHTML='<iframe src="https://www.desdeparaguay.com/fb.php?'+facebook[valor]+'" width="336" height="1200" frameborder="0" scrolling="no"></iframe>';
		}else{
			document.getElementById('fb-radio').innerHTML='';
		}
	}














































