function SintonizarRadioReal(valor,valor2,valor3){
			//document.title=valor2;
			//window.location.hash="#EnUnaNota";
			//document.title="DesdeParaguay.com - Radios de Paraguay";

			//cambiar la url document.location
			//history.pushState(null,"",valor);
			//history.pushState(null,"","https://www.desdeparaguay.com/");

